# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Cafe' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for cafe
  pod 'Apollo'
  pod 'SimpleCheckbox'
  pod 'SnapKit', '~> 5.0.0'


  target 'CafeTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

end
